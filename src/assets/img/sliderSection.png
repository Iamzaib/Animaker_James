    }
    else{
    return true;
    }
  }
  function check_cookie_policy(){
    // console.log("ad cookie check");
    var cookie_bar = getCookie("anim_cookie_policy");
    if (cookie_bar == "" ) {
      return false;
    }
    else{
    return true;
    }
  }
  </script>

