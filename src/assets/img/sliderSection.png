/* ANIMATION ISSUE */

a.rainbow-btn{
  position: relative;
  background: linear-gradient(330deg, #9DFF71 0%, #FFD88D 30%, #FF76D5 65%, #55BEFF 100%);
  /* background-size: 50% 100%; */
  margin: 43px auto 62px auto;
  padding:0;
  width: 320px;
  height: 64px;
  text-align: center;
  color: #ffffff;
  display: block;
  cursor: pointer;
  border-radius: 40px;
  font:normal bold normal 25px/32px 'Inter', sans-serif;
}
a.rainbow-btn:before{
  content: '';
  z-index: 1;
  position: absolute;
  display: block;
  width: 100%;
  height: 70%;
  top: 15%;
  left: 0;
  transition: 0.3s opacity ease-in-out;
  filter: blur(5px);
  opacity: 0;
  background: linear-gradient(330deg, #9DFF71 0%, #FFD88D 30%, #FF76D5 65%, #55BEFF 100%);
  /* background-size: 50% 100%; */
}
a.rainbow-btn:after{
  content: 'Create for Free';
  text-align: center;
  color: white;
  background-color: #5128c6;

  font:normal bold normal 25px/32px 'Inter', sans-serif;
  width: calc(100% - 6px);
  height: calc(100% - 6px);
  top: 3px;
  left: 3px;
  border-radius: 40px;
  z-index: 5;
  position: absolute;
  display: block;
  padding: 10px;
  
}
a.rainbow-btn:focus:before,
a.rainbow-btn:hover:before {
  opacity: 1;
  transition: 0.3s opacity ease-in-out;
  filter: blur(25px);
  background: linear-gradient(330deg, #9DFF71 0%, #FFD88D 30%, #FF76D5 65%, #55BEFF 100%);
  /* background-size: 50% 100%; */
  /* animation: rainbow-btn .75s linear infinite; */
}
a.rainbow-btn:hover{
  text-decoration: none;
  color:white;
}


