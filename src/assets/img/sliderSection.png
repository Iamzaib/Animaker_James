  <script type="text/javascript">
    $(document).ready(function(){
      console.log('Home Page Dom Ready');

      // HIDE HELOBAR BY DEFAULT
      $('.header_helobar_div .helobar').hide();
      $('header.header_area').css('top','0');
      
      // PRODUCT CAROUSAL CONTENT
      const swiper1 = new Swiper('.product-carousal-content .swiper', {
        // Optional parameters
        loop: true,
        allowTouchMove:false,
        speed:1200,
        spaceBetween:30,

        
        // Navigation arrows
        navigation: {
          nextEl: '.swiper-button-next',
          prevEl: '.swiper-button-prev',
        },

      });


      $('.product-carousal-content ul.blog-head > li').on('click', function() {
          $('.product-carousal-content ul.blog-head > li').removeClass('active');
          // console.log($(this).index());
          let clickedIndex = $(this).index();
          swiper1.slideToLoop(clickedIndex);
          setTimeout(function(){
            let currentChild = clickedIndex+1;
            let selector = '.product-carousal-content ul.blog-head > li:nth-child('+currentChild+')';
            $(selector).addClass('active');
          },0)
      });

      // TESTIMONIAL CAROUSAL SECTIONS
      
      const swiper2 = new Swiper('.animaker-testimonials .swiper', {
        // Optional parameters
        loop: true,
        allowTouchMove:false,
        speed:1200,
        spaceBetween:30,
        
        // Navigation arrows
        navigation: {
          nextEl: '.animaker-testimonials .swiper-button-next',
          prevEl: '.animaker-testimonials .swiper-button-prev',
        },

      });

      // BLOG CAROUSAL CTA
      
      const swiper3 = new Swiper('.swiper.blog', {
        // Optional parameters
        // loop: true,
        slidesPerView:3,
        slidesPerGroup:3,
        // slidesPerGroupSkip: 2,
        allowTouchMove:false,
        spaceBetween:30,
        speed:1200,

      });

      $('.blog-link ul.blog-head > li').on('click', function() {
          $('.blog-link ul.blog-head > li').removeClass('active');
          // console.log($(this).index());
          let clickedIndex = $(this).index();
          let nextIndex;
          if (clickedIndex == 0){
            nextIndex = 0;
          }else if(clickedIndex == 1){
            nextIndex = 3;
          }else if(clickedIndex == 2){
            nextIndex = 6;
          }
          swiper3.slideTo(nextIndex);
          setTimeout(function(){
            let currentChild = clickedIndex+1;
            let selector = '.blog-link ul.blog-head > li:nth-child('+currentChild+')';
            $(selector).addClass('active');
          },0)
      });

      AOS.init({
          duration: 1200,
          disable: 'mobile',
        });


      // .product-carousal-content.icon-left-arrow
      // .product-carousal-content.icon-right-arrow

      // PRODUCT CAROUSAL HORIZONTAL NAVIGATION FOR MOBILE
      var button = document.getElementById('prod-next');
        button.onclick = function () {
            var container = document.getElementById('prod-container');
            sideScroll(container,'right',25,100,10);
        };

        var back = document.getElementById('prod-prev');
        back.onclick = function () {
            var container = document.getElementById('prod-container');
            sideScroll(container,'left',25,100,10);
        };

        function sideScroll(element,direction,speed,distance,step){
            scrollAmount = 0;
            var slideTimer = setInterval(function(){
                if(direction == 'left'){
                    element.scrollLeft -= step;
                } else {
                    element.scrollLeft += step;
                }
                scrollAmount += step;
                if(scrollAmount >= distance){
                    window.clearInterval(slideTimer);
                }
            }, speed);
        }
        
        var videoSourceHero = 'https://www.animaker.com/Animaker-Home/new-assets/newhome-loop-video.mp4';
        if ($(window).width() > 768) {
          console.log('Desktop View');
          $('video#hero-video-desktop source').attr('src', videoSourceHero);
          $("video#hero-video-desktop")[0].load();
        }else {
          console.log('Mobile View');
          $('video#hero-video-mobile source').attr('src', videoSourceHero);
          $("video#hero-video-mobile")[0].load();
        }
      

    });
    
  </script>

